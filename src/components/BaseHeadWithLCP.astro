---

const {
  site = "https://centiskor.ch",
  title,
  description,
  lcpMediaAs,
  lcpSrc,
  lcpMediaType,
  canonicalURL = Astro.url
} = Astro.props;

const blogRssFeed = site + "/rss.xml";
---
<head>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>{title}</title>
<link rel="stylesheet" type="text/css" href="/style.css">
<link rel="preload" href="/fonts/Georama_Expanded-Black-subset.woff2" as="font" type="font/woff2" crossorigin="">
<link rel="preload" as="image" href="/images/851.png" type="image/png">
<link rel="preload" as="image" href="/images/Cherish_Ball_Sprite.png" type="image/png">
<link rel="preload" as={lcpMediaAs} href={lcpSrc} type={lcpMediaType}>
<script defer data-domain="centiskor.ch" src="https://plausible.io/js/script.js"></script>
<meta name="description" content={description}>
<link rel="icon" type="image/ico" href="/favicon.ico" />
<meta name="generator" content={Astro.generator} />
<meta property="og:type" content="website">
<meta property="og:title" content={title}>
<meta property="og:description" content={description}>
<meta property="og:url" content={canonicalURL}>
<meta property="og:image" content={site + lcpSrc}>
<link rel="canonical" href={canonicalURL} />
<link rel="alternate" type="application/rss+xml" title="Subscribe to the centiskor.ch blog!" href={blogRssFeed} />
</head>